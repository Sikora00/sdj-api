{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ng": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/ng",
      "sourceRoot": "apps/ng/src",
      "prefix": "sdj",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ng",
            "index": "apps/ng/src/index.html",
            "main": "apps/ng/src/main.ts",
            "polyfills": "apps/ng/src/polyfills.ts",
            "tsConfig": "apps/ng/tsconfig.app.json",
            "assets": [
              "apps/ng/src/favicon.ico",
              "apps/ng/src/env.js",
              "apps/ng/src/assets"
            ],
            "styles": [
              "apps/ng/src/styles.scss",
              "apps/ng/src/styles-app-loading.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng/src/environments/environment.ts",
                  "with": "apps/ng/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng:build",
            "proxyConfig": "apps/ng/proxy.conf.json",
            "host": "0.0.0.0",
            "disableHostCheck": true
          },
          "configurations": {
            "production": {
              "browserTarget": "ng:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng/tsconfig.app.json",
              "apps/ng/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ng/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ng/jest.config.js",
            "tsConfig": "apps/ng/tsconfig.spec.json",
            "setupFile": "apps/ng/src/test-setup.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-e2e": {
      "root": "apps/ng-e2e",
      "sourceRoot": "apps/ng-e2e/src",
      "projectType": "application",
      "architect": {
