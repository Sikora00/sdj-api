{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ng": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/ng",
      "sourceRoot": "apps/ng/src",
      "prefix": "sdj",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ng",
            "index": "apps/ng/src/index.html",
            "main": "apps/ng/src/main.ts",
            "polyfills": "apps/ng/src/polyfills.ts",
            "tsConfig": "apps/ng/tsconfig.app.json",
            "assets": [
              "apps/ng/src/favicon.ico",
              "apps/ng/src/env.js",
              "apps/ng/src/assets"
            ],
            "styles": ["apps/ng/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng/src/environments/environment.ts",
                  "with": "apps/ng/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng:build",
            "proxyConfig": "apps/ng/proxy.conf.json",
            "host": "0.0.0.0",
            "disableHostCheck": true
          },
          "configurations": {
            "production": {
              "browserTarget": "ng:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng/tsconfig.app.json",
              "apps/ng/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ng/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ng/jest.config.js",
            "tsConfig": "apps/ng/tsconfig.spec.json",
            "setupFile": "apps/ng/src/test-setup.ts"
          }
        }
      }
    },
    "ng-e2e": {
      "root": "apps/ng-e2e",
      "sourceRoot": "apps/ng-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-e2e/cypress.json",
            "tsConfig": "apps/ng-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/ng-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**", "!apps/ng-e2e/**"]
          }
        }
      }
    },
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend/src",
      "projectType": "application",
      "prefix": "backend",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/backend",
            "main": "apps/backend/src/main.ts",
            "tsConfig": "apps/backend/tsconfig.app.json",
            "assets": ["apps/backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backend/src/environments/environment.ts",
                  "with": "apps/backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "./builders:ts-node",
          "options": {
            "buildTarget": "backend:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/backend/tsconfig.app.json",
              "apps/backend/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/backend/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/backend/jest.config.js",
            "tsConfig": "apps/backend/tsconfig.spec.json"
          }
        }
      }
    },
    "host-api": {
      "root": "apps/host-api",
      "sourceRoot": "apps/host-api/src",
      "projectType": "application",
      "prefix": "host-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/host-api",
            "main": "apps/host-api/src/main.ts",
            "tsConfig": "apps/host-api/tsconfig.app.json",
            "assets": ["apps/host-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/host-api/src/environments/environment.ts",
                  "with": "apps/host-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "host-api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/host-api/tsconfig.app.json",
              "apps/host-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/host-api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/host-api/jest.config.js",
            "tsConfig": "apps/host-api/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "sdj"
}
