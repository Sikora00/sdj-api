{
  "name": "sdj-backend",
  "version": "1.0.0",
  "description": "Slack DJ backend",
  "main": "./src/main.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "compile": "tsc -p ./tsconfig.json",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "start": "ts-node src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "test": "mocha src/**/*.spec.js -R spec --bail",
    "test:debug": "mocha --inspect-brk=0.0.0.0:58587 src/**/*.spec.js",
    "test:cov": "istanbul cover --dir ./coverage node_modules/mocha/bin/_mocha -- src/**/*.spec.js -R spec --bail && npm run test:remap",
    "test:remap": "remap-istanbul -i ./coverage/coverage.json -o coverage/typescript-report -t html"
  },
  "author": "Patryk Zieli≈Ñski",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.11.0"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/typeorm": "^5.2.2",
    "@nestjs/websockets": "^5.6.2",
    "@slack/client": "^4.3.1",
    "@slack/events-api": "^1.0.1",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.15",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "hbs": "^4.0.1",
    "mysql": "^2.16.0",
    "parse-iso-duration": "^1.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "socket.io": "^2.1.1",
    "typeorm": "^0.2.11",
    "typescript": "^3.0.1",
    "youtube-dl": "^1.12.2"
  }
}
